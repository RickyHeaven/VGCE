{
  "name": "@zhangqingcq/vgce",
  "version": "0.0.17",
  "description": "Vector graphics configure editor. svg组态编辑器。基于vue3.3+ts+element-plus+vite",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "./dist/vgce.umd.cjs",
  "module": "./dist/vgce.js",
  "exports": {
    ".": {
      "import": "./dist/vgce.js",
      "require": "./dist/vgce.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/VGCE.git"
  },
  "keywords": [
    "vue",
    "pinia",
    "echarts",
    "svg",
    "vector",
    "graphics",
    "editor",
    "element",
    "vite",
    "ricky",
    "zhangqingcq"
  ],
  "author": "RickyHeaven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RickyHeaven/VGCE/issues"
  },
  "homepage": "https://github.com/RickyHeaven/VGCE#readme",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "ace-builds": "^1.14.0",
    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "echarts": "^5.4.1",
    "element-plus": "^2.3.7",
    "lodash": "^4.17.21",
    "mqtt": "^4.3.7",
    "pinia": "^2.0.36",
    "vue": "^3.3.4",
    "vue-echarts": "^6.5.1",
    "vue-router": "^4.2.0",
    "vue3-ace-editor": "^2.2.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.0.0",
    "typescript": "~5.1.6",
    "vite": "4.3.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.9.0",
    "vue-tsc": "^1.8.4",
    "windicss": "^3.5.6"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1",
    "pnpm": ">=8.6.0"
  }
}
