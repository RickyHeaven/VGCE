{
  "name": "vgce",
  "version": "0.0.1",
  "description": "Vector graphics configure editor. svg组态编辑器。基于vue3.3+ts+element-plus+vite+webtopo-svg-edit",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib"
  },
  "dependencies": {
    "ace-builds": "^1.14.0",
    "animate.css": "^4.1.1",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.9",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-echarts": "^6.5.1",
    "vue-router": "^4.2.0",
    "vue3-ace-editor": "^2.2.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.9.0",
    "vue-tsc": "^1.6.4",
    "windicss": "^3.5.6"
  }
}
